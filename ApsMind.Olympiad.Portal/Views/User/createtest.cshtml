
@{
    ViewBag.Title = "createtest";
    Layout = "~/Views/Shared/_LayoutNew.cshtml";
}
<div class="mar">
    <!-- PAGE CONTENT WRAPPER -->
    <div class="page-content-wrap">

        <div class="row">
            <div class="col-md-12">

                <form class="form-horizontal">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <h2><span class="fa fa-arrow-circle-o-left" id="pre"></span> Create Your Own Test</h2>
                        </div>
                        <div class="panel-body">
                            @*<div class="alert alert-warning" role="alert">
                                    <button type="button" class="close" data-dismiss="alert"><span aria-hidden="true">×</span><span class="sr-only">Close</span></button>
                                    <strong>Important!</strong> Main feature of this page is "Change photo" function. Press button "Change photo" and try to use this awesome feature.
                                </div>*@
                        </div>
                        <div class="panel-body">
                            <label class="check"><input type="checkbox" class="icheckbox" id="selectall" /><strong>&nbsp;Select All Test Papers</strong> </label><hr />
                            <div id="dvQuizeList">
                                <div class="row">
                                    <div class="col-sm-4">
                                        <div class="form-group">
                                            <div class="checkbox">
                                                <label>
                                                    <input type="checkbox" value="0" name="custQuizIds" id="customeQuiz1" />
                                                    1- iOS 2014 paper type class 1
                                                </label>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                                <div class="row">
                                    <div class="col-sm-4">
                                        <div class="form-group">
                                            <div class="checkbox">
                                                <label>
                                                    <input type="checkbox" value="0" name="custQuizIds" id="customeQuiz2" />
                                                    2- iOS 2014 paper type class 2
                                                </label>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                            </div>
                            <div class=" text-center mgtop30">
                                <div id="dvValidatedQuizSelection" class="col-lg-12 text-danger text-left" style="display:none">
                                    <ul>
                                        <li class="">Please select at least one of the listed Quiz.</li>
                                    </ul>
                                </div>
                                <hr />
                                <div class="row">
                                    <div class="col-sm-6">
                                        <div class="col-sm-6">
                                            <label class=" control-label">Select Difficulty Level</label>
                                        </div>
                                        <div class="col-sm-6">
                                            <input class="exSliderDiffLevel" id="txtdiflevel1" type="text" data-slider-ticks="[0, 1, 2]" data-slider-ticks-snap-bounds="30" data-slider-ticks-labels='["Low", "Medium", "High"]' />
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="col-sm-6">
                                            <label class=" control-label">Select No.of Questions</label>
                                        </div>
                                        <div class="col-sm-6">
                                            <input class="exSliderNoofQuestions" id="txtnooqns1" type="text" data-slider-ticks="[10, 15, 20]" data-slider-ticks-snap-bounds="30" data-slider-ticks-labels='["10", "20", "30"]' />
                                        </div>
                                    </div>
                                </div>
                                <hr />
                                <div class="row col-sm-12">
                                    <a href="#" class="btn btn-primary btn-block btn-rounded" id="btnCreateCustomeQuiz">Create Own Custom Test Paper Name</a>
                                    <br />
                                </div>

                                @*<div id="dialog">
                                        <div class="col-lg-2">

                                            <label id="lblCustomPaperName">Custom Paper Name</label>
                                        </div>
                                        <div class="col-lg-2">

                                            <input type="text" id="TxtCustomPapaerName" />
                                        </div>
                                    </div>*@
                                @*<a href="/User/CustomQuestionPaper" class="btn btn-danger btn-lg" id="StartCustomTest">Start</a>*@
                            </div>
                        </div>

                    </div>
                </form>

            </div>
        </div>

    </div>
    <!-- END PAGE CONTENT WRAPPER -->
    <!-- END PAGE CONTENT -->
    <div class="modal fade" id="modal_custom_name" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title" id="myModalLabel">Custom Test Paper Name</h4>
                </div>
                <div class="modal-body">
                    <form role="form">

                        <div class="form-group">
                            <label for="txtNoteComment">Paper Name<span>*</span></label>
                            <textarea name="txtCutomPaperName" id="txtCutomPaperName" class="form-control"></textarea>
                        </div>
                    </form>
                </div>
                <div id="dvValidatedStatus" class="col-lg-12 text-danger" style="display:block">
                    <ul>
                        <li class="">Please fill required(*) fields</li>
                    </ul>
                </div>
                <div class="modal-footer">

                    <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
                    <button type="button" class="btn btn- danger" id="btnSaveTestName">Start</button>
                </div>
            </div>
        </div>
    </div>
</div>